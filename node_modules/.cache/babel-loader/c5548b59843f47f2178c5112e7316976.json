heck form-check-inline\">\r\n                    <input\r\n                    v-model=\"todo.Category\"\r\n                    class=\"form-check-input\"\r\n                    type=\"checkbox\"\r\n                    id=\"15\"\r\n                    value=\"Civilization\"\r\n                    \r\n                    />\r\n                    <label class=\"form-check-label\" for=\"Civilization\">Civilization</label>\r\n                </div>\r\n\r\n            <br>\r\n            <br>\r\n            <div class=\"mb-3\">\r\n                <label for=\"año\" class=\"form-label\">Año Lanzado</label>\r\n                <input\r\n                v-model.number=\"todo.Year\"\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                id=\"Year\"\r\n                placeholder=\"Ingrese un año\"\r\n                min=\"1000\"\r\n                max=\"9999\"\r\n                />\r\n            </div>\r\n\r\n            <br>\r\n            <button class=\"btn  btn-success\" type=\"submit\">Guardar </button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { nanoid } from 'nanoid'\r\nimport { mapActions } from 'vuex'\r\nexport default {\r\n    \r\n    name: 'Agregar',\r\n    data:()=> {\r\n        return {\r\n            todo:{\r\n                id: '',\r\n                Name: '',\r\n                Publisher: '',\r\n                Category: [],\r\n                Description: '',\r\n                Year: ''\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        ...mapActions(['setTodos']),\r\n         \r\n         guardarTodo() {\r\n             console.log(\"todo\", this.todo);\r\n\r\n             this.todo.id = nanoid(4);\r\n            console.log(\"id\", this.todo.id);\r\n\r\n            this.setTodos(this.todo);\r\n            this.limpiar();\r\n            this.$router.push({name: 'Home'}); //cambia a la vista de home al momento de hacer click en guardar \r\n\r\n         },\r\n         limpiar(){\r\n            this.todo = {\r\n                id: '',\r\n                Name: '',\r\n                Publisher: '',\r\n                Category: [],\r\n                Description: '',\r\n                Year: ''\r\n                }\r\n            }\r\n    },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}